<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_validacao_erro"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="br.com.atan.atancontagem.Registros.ValidacaoErroActivity"
    android:background="@drawable/fundo_app_10">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView15"
        android:background="@drawable/erro"
        android:layout_gravity="center"
        android:layout_marginTop="50dp" />

    <TextView
        android:text="Erro ao validar o código de ativação, consulte a Empresa. "
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView20"
        android:layout_marginStart="10dp"
        android:fontFamily="sans-serif-condensed"
        android:textAlignment="center"
        android:textStyle="normal|bold|italic"
        android:textSize="20sp"
        android:layout_marginEnd="10dp" />

    <Button
        android:text="VOLTAR"
        android:layout_width="match_parent"
        android:id="@+id/btn_voltarApp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="20dp"
        android:textSize="20sp"
        android:layout_height="60dp"
        android:fontFamily="sans-serif-condensed" />
</LinearLayout>
